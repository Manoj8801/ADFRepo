{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "TESTAZUREDATAFACTORY028"
		},
		"LS_AzureDataLakeStorageGEN2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AzureDataLakeStorageGEN2'"
		},
		"ls_AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureBlobStorage1'"
		},
		"ls_AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureSqlDatabase1'"
		},
		"ls_adlsconnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsconnection'"
		},
		"ls_blobconnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blobconnection'"
		},
		"LS_AzureDataLakeStorageGEN2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfl200adlsgen200.dfs.core.windows.net/"
		},
		"ls_adlsconnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfl200adlsgen200.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "TESTAZUREDATAFACTORY028_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureDataLakeStorageGEN2_accountKey": {
						"value": "[parameters('LS_AzureDataLakeStorageGEN2_accountKey')]"
					},
					"ls_AzureBlobStorage1_connectionString": {
						"value": "[parameters('ls_AzureBlobStorage1_connectionString')]"
					},
					"ls_AzureSqlDatabase1_connectionString": {
						"value": "[parameters('ls_AzureSqlDatabase1_connectionString')]"
					},
					"ls_adlsconnection_accountKey": {
						"value": "[parameters('ls_adlsconnection_accountKey')]"
					},
					"ls_blobconnection_connectionString": {
						"value": "[parameters('ls_blobconnection_connectionString')]"
					},
					"LS_AzureDataLakeStorageGEN2_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeStorageGEN2_properties_typeProperties_url')]"
					},
					"ls_adlsconnection_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsconnection_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}